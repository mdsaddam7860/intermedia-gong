{
  "name": "intermedia-gong-integration",
  "version": "1.0.0",
  "description": "Sync call recordings from Intermedia to Gong, including user mapping, call creation, and media upload.",
  "type": "module",
  "main": "src/intermediaToGong.js",
  "scripts": {
    "start": "node src/intermediaToGong.js",
    "dev": "NODE_ENV=development node src/jobs/syncIntermediaToGong.job.js",
    "lint": "echo \"Add eslint\"",
    "clean": "rm -rf temp/intermedia-recordings"
  },
  "bin": {
