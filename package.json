  "author": "Mohammad Saddam",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "base-64": "^1.0.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "form-data": "^4.0.5",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  }
}
